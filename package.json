{
  "name": "svelte-electron-template",
  "version": "1.0.0",
  "description": "Svelte Electron Template",
  "main": "electron.js",
  "scripts": {
    "build": "rollup -c",
    "svelte": "rollup -c -w",
    "start": "sirv public --single",
    "app": "electron .",
    "dev": "concurrently \"npm:svelte\" \"npm:app\""
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "concurrently": "^5.3.0",
    "electron": "^10.1.3",
    "electron-is-dev": "^1.2.0",
    "electron-reload": "^1.5.0",
    "node-sass": "^4.14.1",
    "rollup": "^2.30.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.29.0",
    "svelte-move": "^0.1.27",
    "svelte-preprocess": "^4.5.1"
  },
  "keywords": [
    "svelte"
  ],
  "files": [
    "src",
    "dist"
  ],
  "dependencies": {
    "sirv-cli": "^1.0.6"
  }
}
